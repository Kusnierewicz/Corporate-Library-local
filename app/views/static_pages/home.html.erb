<div class="center jumbotron">
  <h1>Welcome to the mfind Corporate Library</h1>

	<div class="conteiner">
      <% if current_user %>
      <div>
        Signed in as <strong><%= current_user.name %></strong>!
      </div>
        <%= link_to "Sign out", signout_path, id: "sign_out", class: "btn btn-lg btn-primary" %>
      <% else %>
        <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in", class: "btn btn-lg btn-primary" %>
      <% end %>
    </div>
  </div>


